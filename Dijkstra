int dijk(int a, int b, vector<vector<pair<int, int> > > &adj)
{
	priority_queue<pii, vector<pii >, greater<pii > > q;
	vector<bool> vis(V+1, false);
	q.push(MP(0, a));
	while(!q.empty()){
		pii f = q.top();
		q.pop();
		if(vis[f.S])	continue;
		vis[f.S] = true;
		if(f.S == b)	return f.F;
		for(vector<pii >::iterator it = adj[f.S].begin(); it != adj[f.S].end(); it++){
			if(!vis[it->S]){
				q.push(MP(it->F+f.F, it->S));
			}
		}
	}
	return 0;
}
