bool is_prime[MAXN] = {false};
vi primes;
void sieve_eratosthenes()
{
	REP(i,MAXN)	is_prime[i] = true;
	is_prime[1] = false;
	for(int i = 2; i*i <= MAXN; i++){
		if(is_prime[i]){
			for(int j = i*i; j < MAXN; j += i){
				is_prime[j] = false;
			}
		}
	}
	REPc(i,2,MAXN-1){
		if(is_prime[i])
			primes.PB(i);  
	}
}
